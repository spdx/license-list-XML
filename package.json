    },
    "keywords": [
        "SPDX",
        "Software Package Data Exchange",
        "SPDX-License-Identifier",
        "licenses",
        "license",
        "linux foundation",
        "identifiers",
        "oss",
        "open source",
        "software"
    ],
    "scripts": {
        "preinstall": "(npm list gulp -g || npm install gulp -g)"
    },
    "dependencies": {
        "libxml-xsd": "0.5.2",
        "glob": "7.1.2",
        "gulp-util": "3.0.8",
